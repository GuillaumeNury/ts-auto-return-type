{
	"name": "ts-auto-return-type",
	"version": "1.2.0",
	"description": "Use typescript API in order to show functions without return type.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"build": "rimraf dist && tsc",
		"test": "jest",
		"prepare": "npm run build"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"typescript": "^3.9.2"
	},
	"devDependencies": {
		"@types/jest": "^25.2.3",
		"@types/node": "^14.0.1",
		"jest": "^26.0.1",
		"ts-jest": "^26.0.0",
		"ts-node": "^8.10.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"js"
		],
		"transform": {
			"\\.ts$": "ts-jest"
		},
		"testRegex": "/src/.*\\.spec\\.ts$",
		"coverageDirectory": "./coverage/",
		"collectCoverageFrom": [
			"src/**/*.ts"
		],
		"collectCoverage": true
	}
}
